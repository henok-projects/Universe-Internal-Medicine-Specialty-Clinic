{
  "compilerOptions": {
    "jsx": "react",
    "target": "esnext",  
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,  
    "skipLibCheck": true, 
    "strict": false,   
    "module": "esnext",
    "moduleResolution": "node", 
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": ["src"], 
  "exclude": ["node_modules"] 
}